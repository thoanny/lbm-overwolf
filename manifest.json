{
	"manifest_version":1,
	"type":"WebApp",
	"meta":{
		"name":"Le Bus Magique",
		"version":"0.1.0",
		"minimum-overwolf-version":"0.77.10",
		"author":"Anthony Destenay",
		"icon":"IconMouseOver.png",
		"icon_gray":"IconMouseNormal.png",
		"description":"Demo App"
	},
	"data": {
		"start_window":"MainWindow",
		"windows":{
			"MainWindow":{
					"file":"Files/index.html",
					"show_in_taskbar": false,
					"transparent":true,
					"resizable":false,
					"use_os_windowing": false,
					"size":{"width":350, "height":600},
					"min_size":{"width":350, "height":400},
					"max_size":{"width":350, "height":800},
					"topmost": true,
					"disable_rightclick": true,
					"ignore_keyboard_events": true
				},
				"ConfigWindow": {
					"file":"Files/config.html",
					"show_in_taskbar": false,
					"transparent":true,
					"resizable":false,
					"use_os_windowing": false,
					"size":{"width":350, "height":670},
					"topmost": true,
					"disable_rightclick": true
				}
		}
	},
	"permissions": ["Clipboard","FileSystem"]
}
